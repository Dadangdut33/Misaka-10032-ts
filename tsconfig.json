{
	"compilerOptions": {
		"target": "ES6",
		"module": "CommonJS",
		"rootDir": "./src",
		"outDir": "./prod",
		"allowJs": true,
		"strict": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"removeComments": true
	}
}
